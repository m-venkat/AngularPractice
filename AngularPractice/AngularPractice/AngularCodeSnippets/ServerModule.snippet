<CodeSnippet Format="1.1.0" xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <Header>
    <Title>ngServerModuleImport</Title>
    <Author>Venaktesh</Author>
    <Shortcut>ngServerModuleImport</Shortcut>
    <Description>Code snippet to import Angular Browser Module</Description>
    <SnippetTypes>
      <SnippetType>Expansion</SnippetType>
    </SnippetTypes>
  </Header>
  <Snippet>
    <Declarations>
      <Literal>
        <ID>name</ID>
        <ToolTip>Import ServerModule</ToolTip>
        <Default>'ngServerModuleImport'</Default>
      </Literal>
      <Literal>
        <ID>classname</ID>
        <ToolTip>Class Name</ToolTip>
        <Default>BrowserModule</Default>
      </Literal>
    </Declarations>
    <Code Language="TypeScript"><![CDATA[import { ServerModule } from '@angular/platform-server';
    ]]> 
  </Code>
  </Snippet>
</CodeSnippet>